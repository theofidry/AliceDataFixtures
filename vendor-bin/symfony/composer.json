{
    "autoload-dev": {
        "classmap": [
            "../../migrations"
        ]
    },
    "require-dev": {
        "alcaeus/mongo-php-adapter": "^1.0.10",
        "doctrine/data-fixtures": "^1.2",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/mongodb-odm": "^2.1.2",
        "doctrine/mongodb-odm-bundle": "^4.0",
        "doctrine/orm": "^2.5",
        "doctrine/phpcr-bundle": "^2.1.2",
        "doctrine/phpcr-odm": "^1.4",
        "jackalope/jackalope-doctrine-dbal": "^1.2",
        "symfony/symfony": "^4.4 || ^5.3",
        "theofidry/composer-inheritance-plugin": "^1.2",
        "wouterj/eloquent-bundle": "^2.1"
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "provide": {
        "ext-mongo": "*"
    },
    "replace": {
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*"
    },
    "require": {
        "symfony/doctrine-messenger": "^5.3"
    }
}

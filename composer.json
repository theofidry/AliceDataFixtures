{
    "name": "theofidry/alice-data-fixtures",
    "description": "Nelmio alice extension to persist the loaded fixtures.",
    "keywords": ["Fixture", "ORM", "Alice", "Faker", "data", "tests"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Th√©o FIDRY",
            "email": "theo.fidry@gmail.com",
            "homepage": "https://github.com/theofidry"
        }
    ],
    "require": {
        "php": "~7.0.0 || ~7.1.0",
        "nelmio/alice": "^3.0@beta"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.0",
        "phpunit/phpunit": "^5.5",
        "symfony/phpunit-bridge": "^3.1"
    },
    "suggest": {
        "doctrine/orm": "To use Doctrine ORM"
    },
    "autoload": {
        "psr-4": { "Fidry\\AliceDataFixtures\\": "src" }
    },
    "autoload-dev": {
        "psr-4": {
            "Fidry\\AliceDataFixtures\\": [
                "fixtures",
                "tests"
            ]
        }
    },
    "extra": {
        "bamarni-bin": {
            "bin-links": false
        },
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "bin-dir": "bin",
        "sort-packages": true
    }
}
